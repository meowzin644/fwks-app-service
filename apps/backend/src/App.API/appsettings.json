{
    "Serilog": {
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithEnvironmentName"
        ],
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "Microsoft.EntityFramework": "Information",
                "System": "Information"
            }
        },
        "Using": [
            "Serilog.Sinks.Console"
        ],
        "WriteTo": [
            { "Name": "Console" }
        ]
    }
}
